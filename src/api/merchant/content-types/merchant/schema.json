{
  "kind": "collectionType",
  "collectionName": "merchants",
  "info": {
    "singularName": "merchant",
    "pluralName": "merchants",
    "displayName": "Merchant",
    "displayField": "merchant_name"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "market": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::site.site",
      "inversedBy": "merchants"
    },
    "page_title_h1": {
      "type": "string"
    },
    "merchant_name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "merchant_name"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "site_url": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "store_description": {
      "type": "blocks"
    },
    "default_affiliate_link": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "seo_title": {
      "type": "string"
    },
    "seo_description": {
      "type": "text"
    },
    "canonical_url": {
      "type": "string"
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "robots": {
      "type": "string",
      "default": "index,follow"
    },
    "coupons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coupon.coupon",
      "mappedBy": "merchant"
    },
    "pageLayout": {
      "type": "enumeration",
      "default": "coupon",
      "enum": [
        "coupon",
        "blog"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "merchants"
    },
    "active_coupon_title_h2": {
      "type": "string"
    },
    "how_to": {
      "type": "blocks"
    },
    "useful_links": {
      "type": "component",
      "component": "merchant.merchant-useful-links",
      "repeatable": true
    },
    "faqs": {
      "type": "blocks"
    },
    "small_blog_section": {
      "type": "blocks"
    },
    "related_merchants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::merchant.merchant"
    },
    "coupon_order": {
      "type": "json"
    },
    "special_offers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::special-offer.special-offer",
      "mappedBy": "featured_merchant"
    }
  }
}
